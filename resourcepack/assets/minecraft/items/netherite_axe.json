{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/netherite_axe"
        },
        "cases": [
            {
                "when": "smprpg:netherite_hatchet",
                "model": {
                    "type": "model",
                    "model": "tools:item/netherite_hatchet"
                }
            },
            {
                "when": "smprpg:dragonsteel_axe",
                "model": {
                    "type": "model",
                    "model": "tools:item/dragonsteel_axe"
                }
            },
            {
                "when": "smprpg:dragonsteel_hatchet",
                "model": {
                    "type": "model",
                    "model": "tools:item/dragonsteel_hatchet"
                }
            }
        ]
    }
}