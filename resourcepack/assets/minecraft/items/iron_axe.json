{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/iron_axe"
        },
        "cases": [
            {
                "when": "smprpg:iron_hatchet",
                "model": {
                    "type": "model",
                    "model": "tools:item/iron_hatchet"
                }
            },
            {
                "when": "smprpg:steel_axe",
                "model": {
                    "type": "model",
                    "model": "tools:item/steel_axe"
                }
            },
            {
                "when": "smprpg:steel_hatchet",
                "model": {
                    "type": "model",
                    "model": "tools:item/steel_hatchet"
                }
            }
        ]
    }
}