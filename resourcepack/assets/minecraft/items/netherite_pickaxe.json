{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/netherite_pickaxe"
        },
        "cases": [
            {
                "when": "smprpg:boiling_pickaxe",
                "model": {
                    "type": "model",
                    "model": "tools:item/boiling_pickaxe"
                }
            },
            {
                "when": "smprpg:dragonsteel_pickaxe",
                "model": {
                    "type": "model",
                    "model": "tools:item/dragonsteel_pickaxe"
                }
            }
        ]
    }
}