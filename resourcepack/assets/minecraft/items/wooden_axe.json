{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/wooden_axe"
        },
        "cases": [
            {
                "when": "smprpg:copper_axe",
                "model": {
                    "type": "model",
                    "model": "tools:item/copper_axe"
                }
            },
            {
                "when": "smprpg:wooden_hatchet",
                "model": {
                    "type": "model",
                    "model": "tools:item/wooden_hatchet"
                }
            },
            {
                "when": "smprpg:copper_hatchet",
                "model": {
                    "type": "model",
                    "model": "tools:item/copper_hatchet"
                }
            }
        ]
    }
}